弹出内容\n      </template>\n    </g-popover>\n    <g-popover position=\"right\">\n      <g-button>右边弹出</g-button>\n      <template slot=\"content\">\n        弹出内容\n      </template>\n    </g-popover>\n\n    <p>\n      <strong>代码</strong>\n    </p>\n    <pre><code>{{content}}</code></pre>\n    <br>\n    <hr>\n    \n    <h3>将触发方式改为 hover</h3>\n    <p>\n      <strong>预览</strong>\n    </p>\n\n    <g-popover trigger=\"hover\">\n      <g-button>上方弹出</g-button>\n      <template slot=\"content\">\n        弹出内容\n      </template>\n    </g-popover>\n    <g-popover position=\"bottom\" trigger=\"hover\">\n      <g-button>下方弹出</g-button>\n      <template slot=\"content\">\n        弹出内容\n      </template>\n    </g-popover>\n    <g-popover position=\"left\" trigger=\"hover\">\n      <g-button>左边弹出</g-button>\n      <template slot=\"content\">\n        弹出内容\n      </template>\n    </g-popover>\n    <g-popover position=\"right\" trigger=\"hover\">\n      <g-button>右边弹出</g-button>\n      <template slot=\"content\">\n        弹出内容\n      </template>\n    </g-popover>\n\n    <p>\n      <strong>代码</strong>\n    </p>\n    <pre><code>{{content1}}</code></pre>\n  </div>\n</template>\n<style>\n</style>\n<style scoped>\n</style>\n<script>\n  import Button from '../../../src/components/button/button'\n  import Popover from '../../../src/components/popover'\n\n\n  export default {\n    components: {\n      'g-button': Button, \n      'g-popover': Popover\n    },\n    data () {\n      return {\n        content: `\n          <g-popover>\n            <g-button>上方弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n          <g-popover position=\"bottom\">\n            <g-button>下方弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n          <g-popover position=\"left\">\n            <g-button>左边弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n          <g-popover position=\"right\">\n            <g-button>右边弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n      `.replace(/^ {8}/gm, '').trim(),\n        content1: `\n          <g-popover trigger=\"hover\">\n            <g-button>上方弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n          <g-popover position=\"bottom\" trigger=\"hover\">\n            <g-button>下方弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n          <g-popover position=\"left\" trigger=\"hover\">\n            <g-button>左边弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n          <g-popover position=\"right\" trigger=\"hover\">\n            <g-button>右边弹出</g-button>\n            <template slot=\"content\">\n              弹出内容\n            </template>\n          </g-popover>\n      `.replace(/^ {8}/gm, '').trim()\n      }\n    },\n    methods: {\n      onClickButton () {\n        this.$toast('<strong style=\"color:red;\">加粗的提示</strong>', {\n          enableHtml: true\n        })\n      }\n    },\n  }\n</script>"]}]}